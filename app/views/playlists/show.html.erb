
<h1><%= @playlist.name %></h1>


<ul>

  <% @tracks_enhanced.each do |t| %>
    <li>
      <% track = t[:track] %>
      <%= link_to track.name, track_path(track.id) %> |
      <%= l(t[:added_at]) %> |
      <%# p.context_type %>
